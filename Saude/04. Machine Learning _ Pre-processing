, 6))\n","    for cnt in range(4):\n","        plt.subplot(2, 2, cnt+1)\n","        for lab in ('Iris-setosa', 'Iris-versicolor', 'Iris-virginica'):\n","            plt.hist(X[y==lab, cnt],\n","                     label=lab,\n","                     bins=10,\n","                     alpha=0.3,)\n","        plt.xlabel(feature_dict[cnt])\n","    plt.legend(loc='upper right', fancybox=True, fontsize=8)\n","\n","    plt.tight_layout()\n","    plt.show()"],"execution_count":0,"outputs":[{"output_type":"display_data","data":{"image/png":"iVBORw0KGgoAAAANSUhEUgAAAjAAAAGoCAYAAABYLN/cAAAABHNCSVQICAgIfAhkiAAAAAlwSFlz\nAAALEgAACxIB0t1+/AAAADl0RVh0U29mdHdhcmUAbWF0cGxvdGxpYiB2ZXJzaW9uIDMuMC4zLCBo\ndHRwOi8vbWF0cGxvdGxpYi5vcmcvnQurowAAIABJREFUeJzs3XtcVHX+P/AXMKICXgAFQ9Fa8zJq\nlrcSbykhrtrFbAvjAeZ+28oU0dLSVFbLyoXMTK1MVCzIYiMz2mpxbWPXrwEltrroGGqX32E0BOR+\niWGY3x98mUDmcubMmcuB1/MfmXP5fN7ncDjz9nPO5/PxMBgMBhAREREpiKerAyAiIiKyFRMYIiIi\nUhwmMERERKQ4TGCIiIhIcZjAEBERkeKoHFVwfn6+o4omIheaMGGCq0Pg/YWok7Ll/uKwBAZw7I1O\no9FArVY7rHzWzbpZd8e63SlxEHt/ceU5tJdSY1dq3IByY1dq3MBvsdt6f+EjJCIiIlIcJjBERESk\nOExgiIiISHFEJTCFhYWIiIhAWloaAODKlStYsmQJYmJisGTJEpSUlDg0SCIiIqK2rCYwdXV12LJl\nC8LCwozLduzYgYceeghpaWmYPXs2UlJSHBokERERUVtWExhvb28kJycjKCjIuGzTpk2YM2cOAMDf\n3x8VFRWOi5CIiIjoOlYTGJVKhR49erRb5uPjAy8vL+j1ehw6dAj33HOPwwIkIiIiup7kcWD0ej2e\nffZZTJ48ud3jpbY0Go3kwKxpaGhwaPm5Qq3ZdbpGHXKFkwCAyaG+DovBFEcfN+tm3e5QNynbyfKT\nKBaKJe07M3SmvMFQpyU5gXnuuecwZMgQxMXFmd3GkYPqOHrQHq3B/B+fUCQgdFAoAECtDnZYDKa4\n28BmrLtr1e1OA9kRUdcmqRt1ZmYmunXrhvj4eLnjISIiIrLKagtMQUEBEhMTodVqoVKpkJWVhbKy\nMnTv3h2xsbEAgKFDh2Lz5s2OjpWIiIgIgIgEZsyYMUhNTXVGLERERESicCReIiIiUhwmMERERKQ4\nTGCIyGGun4Zk3bp1uOeeexAbG4vY2FhkZ2d32Ofll19GVFQUFi1ahDNnzjg5YiJSCsndqImILDE1\nDQkAPP3005g1a5bJfb755hv8/PPPSE9Px6VLl7B+/Xqkp6c7I1wiUhi2wBCRQ5iahsSanJwcRERE\nAGjp3VhZWYmamhpHhUhECsYWGCJyCJVKBZWq4y0mLS0NKSkpCAwMREJCAgICAozrSktLMXr0aOPn\ngIAAlJSUwM/Pr0M5YkcJVvKIwkqNXafTQRAESftqauQ5XkujqVvSdqT1Vs4ecV0KpV4rgPTYmcAQ\nkdPcd9996Nu3L9RqNfbu3Yvdu3fjz3/+s9ntDQaD2XViRyh25WjG9lJq7